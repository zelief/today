{
  "name": "strive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "dotenv -e .env.test jest",
    "test:full": "dotenv -e .env.test yarn docker:up && yarn migrate:test && yarn test",
    "migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init ",
    "docker:up": "docker-compose up --wait --remove-orphans -d",
    "docker:down": "docker-compose down",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "studio": "dotenv -e .env.local -- npx prisma studio",
    "studio:test": "dotenv -e .env.test -- npx prisma studio",
    "ts": "node_modules/.bin/tsc --noEmit --watch "
  },
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.27.0",
    "eslint-config-next": "^13.1.1",
    "next": "^13.1.1",
    "prisma": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.2",
    "ts-jest": "^29.0.3"
  }
}
